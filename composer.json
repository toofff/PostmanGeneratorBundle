{
    "name": "api-platform/postman-collection-generator",
    "type": "symfony-bundle",
    "description": "Generator for Postman collection based on API Platform 1.*",
    "keywords": ["Postman", "API Platform"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Chalamon",
            "email": "vincentchalamon@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/console": "^2.3|^3.0",
        "symfony/dependency-injection": "^2.3|^3.0",
        "symfony/config": "^2.3|^3.0",
        "symfony/property-access": "^3.0",
        "doctrine/inflector": "^1.1",
        "dunglas/api-bundle": "~1.1@beta",
        "ramsey/uuid": "^3.2",
        "fzaninotto/faker": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "<5.0",
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.0",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2"
    },
    "autoload": {
        "psr-4": {
            "PostmanGeneratorBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PostmanGeneratorBundle\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
